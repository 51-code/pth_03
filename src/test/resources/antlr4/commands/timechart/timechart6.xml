<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
    <transformStatement>
        <value>|</value>
        <timechartTransformation>
            <value>timechart</value>
            <t_timechart_sepParameter>
                <value>sep=</value>
                <stringType>
                    <value>yyy</value>
                </stringType>
            </t_timechart_sepParameter>
            <t_timechart_formatParameter>
                <value>format=</value>
                <stringType>
                    <value>cool</value>
                </stringType>
            </t_timechart_formatParameter>
            <t_timechart_partialParameter>
                <value>partial=</value>
                <booleanType>
                    <value>FALSE</value>
                </booleanType>
            </t_timechart_partialParameter>
            <t_timechart_limitParameter>
                <value>limit=</value>
                <value>top4</value>
            </t_timechart_limitParameter>
            <t_timechart_fixedrangeParameter>
                <value>fixedrange=</value>
                <booleanType>
                    <value>TRUE</value>
                </booleanType>
            </t_timechart_fixedrangeParameter>
            <t_timechart_binOptParameter>
                <t_timechart_spanOptParameter>
                    <t_timechart_binSpanParameter>
                        <value>span=</value>
                        <spanType>
                            <value>8log6</value>
                        </spanType>
                    </t_timechart_binSpanParameter>
                </t_timechart_spanOptParameter>
            </t_timechart_binOptParameter>
            <aggregateFunction>
                <aggregateMethodAvg>
                    <value>avg(</value>
                    <aggregate_fieldType>
                        <value>hhh</value>
                    </aggregate_fieldType>
                    <value>)</value>
                </aggregateMethodAvg>
            </aggregateFunction>
            <t_timechart_divideByInstruction>
                <value>BY</value>
                <fieldType>
                    <value>host</value>
                </fieldType>
                <t_timechart_tcOpt>
                    <t_timechart_useotherParameter>
                        <value>useother=</value>
                        <booleanType>
                            <value>TRUE</value>
                        </booleanType>
                    </t_timechart_useotherParameter>
                </t_timechart_tcOpt>
                <t_timechart_tcOpt>
                    <t_timechart_nullstrParameter>
                        <value>nullstr=</value>
                        <stringType>
                            <value>hhh</value>
                        </stringType>
                    </t_timechart_nullstrParameter>
                </t_timechart_tcOpt>
                <t_timechart_tcOpt>
                    <t_timechart_otherstrParameter>
                        <value>otherstr=</value>
                        <stringType>
                            <value>iii</value>
                        </stringType>
                    </t_timechart_otherstrParameter>
                </t_timechart_tcOpt>
            </t_timechart_divideByInstruction>
        </timechartTransformation>
        <transformStatement>
            <EOF/>
        </transformStatement>
    </transformStatement>
</root>
